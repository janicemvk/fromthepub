---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 <title>Adam Wilcox</title>
 <link href="http://adamwilcox.org/atom.xml" rel="self"/>
 <link href="http://adamwilcox.org/"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://adamwilcox.org/</id>
 <author>
   <name>Adam Wilcox</name>
 </author>
 {% for post in site.posts limit:10 %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://adamwilcox.org{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://adamwilcox.org{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
   <author>
     <name>Adam Wilcox</name>
     <uri>http://adamwilcox.org/about/</uri>
   </author>
 </entry>
 {% endfor %}
</feed>